{
  "extends": "./configs/nextjs.json",
  "compilerOptions": {
    "target": "es2017",
    "baseUrl": ".",
    "noEmit": true,
    "jsx": "preserve",
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "src/**/*",
    "index.d.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    ".next"
  ]
}
